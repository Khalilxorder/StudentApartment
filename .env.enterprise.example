# Environment Variables for Enterprise Features

# Redis (Upstash) - Required for production rate limiting
UPSTASH_REDIS_REST_URL=https://your-redis-url.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_redis_token_here

# Snyk (Optional) - For CI/CD security scanning
SNYK_TOKEN=your_snyk_token_here

# PagerDuty (Optional) - For incident management
PAGERDUTY_API_KEY=your_pagerduty_key_here
PAGERDUTY_SERVICE_ID=your_service_id_here

#PostHog (Optional) - For analytics
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_key_here
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Feature Flags (Optional)
FEATURE_FLAGS_ENABLED=true

# API Versioning
API_VERSION=v2
